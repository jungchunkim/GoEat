<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.GOEAT.Go_Eat.AnalysisHomeRealActivity">

    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.GOEAT.Go_Eat.widget.MeowBottomNavigationWrapper
        android:id="@+id/bottom_nav_wrapper"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true">

        <com.etebarian.meowbottomnavigation.MeowBottomNavigation
            android:id="@+id/bottom_nav"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mbn_backgroundBottomColor="#ffffff"
            app:mbn_circleColor="@color/mainPink"
            app:mbn_countBackgroundColor="#ff6f66"
            app:mbn_defaultIconColor="#99000000"
            app:mbn_selectedIconColor="#FFFFFF" />

    </com.GOEAT.Go_Eat.widget.MeowBottomNavigationWrapper>
</RelativeLayout>